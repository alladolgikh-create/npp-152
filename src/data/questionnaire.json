    { "id": 81, "block": 1, "subscale": "reward_dependence", "facet": "RD3_selfdisclosure", "text": "Делюсь сокровенными мыслями", "reversed": false },
    { "id": 82, "block": 1, "subscale": "reward_dependence", "facet": "RD3_selfdisclosure", "text": "Показываю свои чувства", "reversed": false },
    { "id": 83, "block": 1, "subscale": "reward_dependence", "facet": "RD3_selfdisclosure", "text": "Рассказываю о своих тревогах", "reversed": false },
    { "id": 84, "block": 1, "subscale": "reward_dependence", "facet": "RD3_selfdisclosure", "text": "Мало разговариваю", "reversed": true },
    { "id": 85, "block": 1, "subscale": "reward_dependence", "facet": "RD3_selfdisclosure", "text": "Мало рассказываю о себе", "reversed": true },
    { "id": 86, "block": 1, "subscale": "reward_dependence", "facet": "RD3_selfdisclosure", "text": "Держу чувства в себе", "reversed": true },
    { "id": 87, "block": 1, "subscale": "reward_dependence", "facet": "RD3_selfdisclosure", "text": "Мне мало что есть сказать", "reversed": true },
    { "id": 88, "block": 1, "subscale": "reward_dependence", "facet": "RD3_selfdisclosure", "text": "Говорю мало", "reversed": true },
    { "id": 89, "block": 1, "subscale": "reward_dependence", "facet": "RD4_dependence", "text": "Стараюсь угодить всем", "reversed": false },
    { "id": 90, "block": 1, "subscale": "reward_dependence", "facet": "RD4_dependence", "text": "Следую указаниям", "reversed": false },
    { "id": 91, "block": 1, "subscale": "reward_dependence", "facet": "RD4_dependence", "text": "Делаю то, что другие хотят от меня", "reversed": false },
    { "id": 92, "block": 1, "subscale": "reward_dependence", "facet": "RD4_dependence", "text": "Не люблю казаться напористым(ой)", "reversed": false },
    { "id": 93, "block": 1, "subscale": "reward_dependence", "facet": "RD4_dependence", "text": "Выполняю приказы", "reversed": false },
    { "id": 94, "block": 1, "subscale": "reward_dependence", "facet": "RD4_dependence", "text": "Мне все равно, что думают другие", "reversed": true },
    { "id": 95, "block": 1, "subscale": "reward_dependence", "facet": "RD4_dependence", "text": "Быстро распознаю возможности", "reversed": true },
    { "id": 96, "block": 1, "subscale": "reward_dependence", "facet": "RD4_dependence", "text": "Никому не уступаю", "reversed": true },
    { "id": 97, "block": 1, "subscale": "reward_dependence", "facet": "RD4_dependence", "text": "Хочу отличаться от других", "reversed": true },
    { "id": 98, "block": 1, "subscale": "reward_dependence", "facet": "RD4_dependence", "text": "Верю только в себя", "reversed": true },
    { "id": 99, "block": 2, "subscale": "stai", "text": "Я испытываю удовольствие", "reversed": true },
    { "id": 100, "block": 2, "subscale": "stai", "text": "Я очень быстро устаю", "reversed": false },
    { "id": 101, "block": 2, "subscale": "stai", "text": "Я легко могу заплакать", "reversed": false },
    { "id": 102, "block": 2, "subscale": "stai", "text": "Я хотел бы быть таким же счастливым, как и другие", "reversed": false },
    { "id": 103, "block": 2, "subscale": "stai", "text": "Нередко я проигрываю из-за того, что недостаточно быстро принимаю решения", "reversed": false },
    { "id": 104, "block": 2, "subscale": "stai", "text": "Обычно я чувствую себя бодрым", "reversed": true },
    { "id": 105, "block": 2, "subscale": "stai", "text": "Я спокоен, хладнокровен и собран", "reversed": true },
    { "id": 106, "block": 2, "subscale": "stai", "text": "Ожидаемые трудности обычно очень тревожат меня", "reversed": false },
    { "id": 107, "block": 2, "subscale": "stai", "text": "Я слишком переживаю из-за пустяков", "reversed": false },
    { "id": 108, "block": 2, "subscale": "stai", "text": "Я вполне счастлив", "reversed": true },
    { "id": 109, "block": 2, "subscale": "stai", "text": "Я принимаю все слишком близко к сердцу", "reversed": false },
    { "id": 110, "block": 2, "subscale": "stai", "text": "Мне не хватает уверенности в себе", "reversed": false },
    { "id": 111, "block": 2, "subscale": "stai", "text": "Обычно я чувствую себя в безопасности", "reversed": true },
    { "id": 112, "block": 2, "subscale": "stai", "text": "Я стараюсь избегать критических ситуаций и трудностей", "reversed": false },
    { "id": 113, "block": 2, "subscale": "stai", "text": "У меня бывает хандра", "reversed": false },
    { "id": 114, "block": 2, "subscale": "stai", "text": "Я доволен", "reversed": true },
    { "id": 115, "block": 2, "subscale": "stai", "text": "Всякие пустяки отвлекают и волнуют меня", "reversed": false },
    { "id": 116, "block": 2, "subscale": "stai", "text": "Я так сильно переживаю свои разочарования, что потом долго не могу о них забыть", "reversed": false },
    { "id": 117, "block": 2, "subscale": "stai", "text": "Я уравновешенный человек", "reversed": true },
    { "id": 118, "block": 2, "subscale": "stai", "text": "Меня охватывает сильное беспокойство, когда я думаю о своих делах и заботах", "reversed": false },
    { "id": 119, "block": 3, "subscale": "bas", "facet": "BAS_drive", "text": "Я стараюсь изо всех сил, чтобы получить то, что хочу", "reversed": false },
    { "id": 120, "block": 3, "subscale": "bas", "facet": "BAS_drive", "text": "Если я чего-то хочу, я обычно иду напролом, чтобы это получить", "reversed": false },
    { "id": 121, "block": 3, "subscale": "bas", "facet": "BAS_drive", "text": "Если я вижу возможность получить то, что хочу, я тут же за нее хватаюсь", "reversed": false },
    { "id": 122, "block": 3, "subscale": "bas", "facet": "BAS_drive", "text": "Когда я чего-то добиваюсь, для меня нет запретных способов", "reversed": false },
    { "id": 123, "block": 3, "subscale": "bas", "facet": "BAS_fun", "text": "Я всегда готов(а) попробовать что-то новенькое, если думаю, что это будет интересно", "reversed": false },
    { "id": 124, "block": 3, "subscale": "bas", "facet": "BAS_fun", "text": "Я часто делаю что-либо лишь для развлечения", "reversed": false },
    { "id": 125, "block": 3, "subscale": "bas", "facet": "BAS_fun", "text": "Я часто действую по первому порыву", "reversed": false },
    { "id": 126, "block": 3, "subscale": "bas", "facet": "BAS_fun", "text": "Я страстно жажду новых ощущений и того, что вызывает возбуждение", "reversed": false },
    { "id": 127, "block": 3, "subscale": "bas", "facet": "BAS_reward", "text": "Мне нравится делать то, что у меня хорошо получается", "reversed": false },
    { "id": 128, "block": 3, "subscale": "bas", "facet": "BAS_reward", "text": "Когда я получаю то, что хотел(а), я чувствую возбуждение и прилив энергии", "reversed": false },
    { "id": 129, "block": 3, "subscale": "bas", "facet": "BAS_reward", "text": "Я сразу прихожу в возбуждение, когда вижу возможность осуществить свое желание", "reversed": false },
    { "id": 130, "block": 3, "subscale": "bas", "facet": "BAS_reward", "text": "Если со мной случается что-то хорошее, это сильно на меня действует", "reversed": false },
    { "id": 131, "block": 3, "subscale": "bas", "facet": "BAS_reward", "text": "Победа в соревновании сильно поднимает мне настроение", "reversed": false },
    { "id": 132, "block": 3, "subscale": "bis", "text": "Я редко испытываю страх или нервозность, даже если со мной происходит что-то плохое", "reversed": true },
    { "id": 133, "block": 3, "subscale": "bis", "text": "Критика или наказание сильно ранят меня", "reversed": false },
    { "id": 134, "block": 3, "subscale": "bis", "text": "Если я думаю, что кто-то на меня злится, я сильно расстраиваюсь и беспокоюсь", "reversed": false },
    { "id": 135, "block": 3, "subscale": "bis", "text": "Если я думаю, что должно случиться что-то неприятное, я обычно сильно нервничаю", "reversed": false },
    { "id": 136, "block": 3, "subscale": "bis", "text": "Я беспокоюсь, если думаю, что допустил(а) много ошибок в важном деле", "reversed": false },
    { "id": 137, "block": 3, "subscale": "bis", "text": "По сравнению с друзьями у меня очень мало страхов", "reversed": true },
    { "id": 138, "block": 3, "subscale": "bis", "text": "Я беспокоюсь о том, как бы не ошибиться в чем-либо", "reversed": false },
    { "id": 139, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие просмотр любимой теле- или радиопередачи", "reversed": false },
    { "id": 140, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие проводить время с семьей или близкими друзьями", "reversed": false },
    { "id": 141, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие мое хобби или увлечение", "reversed": false },
    { "id": 142, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие моя любимая еда", "reversed": false },
    { "id": 143, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие теплая ванна или освежающий душ", "reversed": false },
    { "id": 144, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие запах цветов, свежевыпеченного хлеба или дуновение морского ветра", "reversed": false },
    { "id": 145, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие видеть других людей улыбающимися", "reversed": false },
    { "id": 146, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие выглядеть умным или произвести впечатление своим появлением", "reversed": false },
    { "id": 147, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие чтение книги, журнала или газеты", "reversed": false },
    { "id": 148, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие чашка чая, или кофе, или моего любимого напитка", "reversed": false },
    { "id": 149, "block": 4, "subscale": "shaps", "text": "Мне доставляют удовольствие обычные вещи — яркий солнечный день, звонок друга", "reversed": false },
    { "id": 150, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие красивый ландшафт или вид", "reversed": false },
    { "id": 151, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие помогать другим людям", "reversed": false },
    { "id": 152, "block": 4, "subscale": "shaps", "text": "Мне доставляет удовольствие похвала от других людей", "reversed": false }
  ]
}
